class Overload{
    int add(int a,int b){
        return a+b;
    }
    int add(int a,int b,int c){
        return a+b+c;
    }
}
class Main {
    public static void main(String[] args) {
        Overload load = new Overload();

        System.out.println("Sum of 2 Number: " + load.add(5, 10));
        System.out.println("Sum of 3 Number: " + load.add(5, 10, 15));
    }
}
