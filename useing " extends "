class Animal {
   void makeSound(){
       System.out.println("Some generic sound");
   }
}

class Dog extends Animal{
   void makeSound(){
       System.out.println("Dogs Bark");
   }
}

class Cat extends Animal{
   void makeSound(){
       System.out.println("Cat meows");
   }
}

public class Main{
   public static void main(String[] args){
       Dog d=new Dog();
       Cat c=new Cat();

       d.makeSound();
       c.makeSound();
   }
}
