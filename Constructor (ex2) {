class Car {
    String make;
    String model;
    int yearOfManufacture;
    String color;
    String fuelType;
    String engineType;
    double price;

    Car(String make, String model, int yearOfManufacture) {
        this.make = make;
        this.model = model;
        this.yearOfManufacture = yearOfManufacture;
    }

    Car(String make, String model, int yearOfManufacture, String color) {
        this.make = make;
        this.model = model;
        this.yearOfManufacture = yearOfManufacture;
        this.color = color;

    }

    Car(String make, String model, int yearOfManufacture, String color, String fuelType) {
        this.make = make;
        this.model = model;
        this.yearOfManufacture = yearOfManufacture;
        this.color = color;
        this.fuelType = fuelType;
    }

    Car(String make, String model, int yearOfManufacture, String color, String fuelType, String engineType) {
        this.make = make;
        this.model = model;
        this.yearOfManufacture = yearOfManufacture;
        this.color = color;
        this.fuelType = fuelType;
        this.engineType = engineType;
    }


    Car(String make, String model, int yearOfManufacture, String color, String fuelType, String engineType, double price) {
        this.make = make;
        this.model = model;
        this.yearOfManufacture = yearOfManufacture;
        this.color = color;
        this.fuelType = fuelType;
        this.engineType = engineType;
        this.price = price;
    }


    public void printDetails() {

        System.out.println("Make: " + make);
        System.out.println("Model: " + model);
        System.out.println("Year: " + yearOfManufacture);
        System.out.println("Color: " + color);
        System.out.println("Fuel Type: " + fuelType);
        System.out.println("Engine Type: " + engineType);
        System.out.println("Price: $" + price);
    }

    public static void main(String[] args) {
        Car car1 = new Car("Toyota", "Camry", 2022);
        Car car2 = new Car("Honda", "Civic", 2023, "Red"+ "\n");
        Car car3 = new Car("Ford", "Mustang", 2024, "Blue", "Electric" + "\n");
        Car car4 = new Car("Tesla", "Model S", 2025, "Black", "Electric", "Electric"+ "\n");
        Car car5 = new Car("BMW", "X5", 2023, "White", "Diesel", "V8", 60000.00);

        car1.printDetails();
        car2.printDetails();
        car3.printDetails();
        car4.printDetails();
        car5.printDetails();
    }
